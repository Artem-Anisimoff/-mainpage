	<script>
	var arr_src = ['./1/kar1.png', './1/kar2.png'];
  var i = 0;
  function swap_img(){
   var img1 = document.getElementById('img1'); 
   img1.setAttribute('src', arr_src[i]);
   if (i < arr_src.length - 1)
     i++;
   else
     i = 0;
 }
  
var cities = {
  napr1: ["Программная инженерия", "Информационные системы"],
  napr2: ["Медиапроектирование и графический дизайн", "Проектирование городской среды"],
  
};
var napr = document.getElementById("napr");
var city = document.querySelector("#city");
window.onload = selectNapr;
napr.onchange = selectNapr;

function selectNapr(ev){
  city.innerHTML = "";
  var c = this.value || "napr1", o;
  for(let i = 0; i < cities[c].length; i++){
    o = new Option(cities[c][i],i,false,false);
    city.add(o);
  };
}
